{
  "expo": {
    "name": "Simple HIIT",
    "slug": "simple-hiit",
    "version": "1.0.8",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/splash-image.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a2e"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.simplehiit.app",
      "buildNumber": "8",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1a1a2e"
      },
      "package": "com.simplehiit.app",
      "versionCode": 8,
      "permissions": [
        "android.permission.VIBRATE",
        "android.permission.WAKE_LOCK"
      ],
      "config": {
        "usesCleartextTraffic": false
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font"
      ],
      [
        "expo-av",
        {
          "microphonePermission": false
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "12345678-1234-1234-1234-123456789abc"
      }
    },
    "owner": "anonymous",
    "updates": {
      "url": "https://u.expo.dev/12345678-1234-1234-1234-123456789abc"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
